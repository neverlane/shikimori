{
  "name": "shikimori",
  "version": "1.0.2",
  "description": "Modern ES6 Promise based API client for shikimori.one",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "docs": "typedoc --out docs src/index.ts --gitRevision nemiga"
  },
  "homepage": "https://github.com/neverlane/shikimori",
  "repository": {
    "type": "git",
    "url": "https://github.com/neverlane/shikimori.git"
  },
  "keywords": [
    "shikimori",
    "anime",
    "api",
    "modern",
    "promise",
    "typescript",
    "wrapper"
  ],
  "author": "neverlane",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "1.2.1",
    "inspectable": "^1.2.2",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.8",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "tsup": "^6.5.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  }
}
